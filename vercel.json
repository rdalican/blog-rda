{
    "version": 2,
    "builds": [
        {
            "src": "api/vercel.py",
            "use": "@vercel/python",
            "config": { 
                "maxLambdaSize": "10mb",
                "runtime": "python3.9"
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/api/vercel.py"
        }
    ],
    "env": {
        "PYTHONPATH": "/var/task"
    }
} 